{
  "name": "graphql-boilerplate",
  "version": "0.1.0",
  "engines": {
    "node": ">=10.0.0"
  },
  "description": "Boilerplate for a GraphQL Apollo Server",
  "main": "src/server.js",
  "scripts": {
    "postinstall": "link-module-alias",
    "dev": "nodemon --exec \"npm run lint && babel-node src/server.js js, graphql\"",
    "lint": "eslint src/**/*.js",
    "babel-node": "babel-node --presets=/*a*/"
  },
  "keywords": [],
  "author": "Dutch Concepts",
  "dependencies": {
    "apollo-server-express": "^2.3.1",
    "chalk": "^4.0.0",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.17.1",
    "graphql-tools": "^5.0.0",
    "lodash.merge": "^4.6.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.5",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-node": "^11.1.0",
    "link-module-alias": "^1.2.0",
    "nodemon": "^1.18.9",
    "prettier": "2.0.5"
  },
  "_moduleAliases": {
    "~": "src"
  }
}
